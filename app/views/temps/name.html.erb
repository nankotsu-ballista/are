<h1>テンプレートを作ろう</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @newtemp, url: set_tempting_false_users_path, method: :post) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :image %>
      <%= f.file_field :image %>

      <%= f.submit "完成", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>